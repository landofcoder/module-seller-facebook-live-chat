<?xml version="1.0" ?>
<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
	<argument name="data" xsi:type="array">
		<item name="js_config" xsi:type="array">
			<item name="provider" xsi:type="string">lofmp_facebooklivechat_sellerfacebook_listing.lofmp_facebooklivechat_sellerfacebook_listing_data_source</item>
		</item>
	</argument>
	<settings>
		<spinner>lofmp_facebooklivechat_sellerfacebook_columns</spinner>
		<deps>
			<dep>lofmp_facebooklivechat_sellerfacebook_listing.lofmp_facebooklivechat_sellerfacebook_listing_data_source</dep>
		</deps>
		<buttons>
			<button name="add">
				<url path="*/*/new"/>
				<class>primary</class>
				<label translate="true">Add new Sellerfacebook</label>
			</button>
		</buttons>
	</settings>
	<dataSource name="lofmp_facebooklivechat_sellerfacebook_listing_data_source" component="Magento_Ui/js/grid/provider">
		<settings>
			<storageConfig>
				<param name="indexField" xsi:type="string">sellerfacebook_id</param>
			</storageConfig>
			<updateUrl path="mui/index/render"/>
		</settings>
		<aclResource>Lofmp_FacebookLiveChat::Sellerfacebook</aclResource>
		<dataProvider name="lofmp_facebooklivechat_sellerfacebook_listing_data_source" class="Magento\Framework\View\Element\UiComponent\DataProvider\DataProvider">
			<settings>
				<requestFieldName>id</requestFieldName>
				<primaryFieldName>sellerfacebook_id</primaryFieldName>
			</settings>
		</dataProvider>
	</dataSource>
	<listingToolbar name="listing_top">
		<settings>
			<sticky>true</sticky>
		</settings>
		<bookmark name="bookmarks"/>
		<columnsControls name="columns_controls"/>
		<filters name="listing_filters"/>
		<paging name="listing_paging"/>
	</listingToolbar>
	<columns name="lofmp_facebooklivechat_sellerfacebook_columns">
		<settings>
			<editorConfig>
				<param name="selectProvider" xsi:type="string">lofmp_facebooklivechat_sellerfacebook_listing.lofmp_facebooklivechat_sellerfacebook_listing.lofmp_facebooklivechat_sellerfacebook_columns.ids</param>
				<param name="enabled" xsi:type="boolean">true</param>
				<param name="indexField" xsi:type="string">sellerfacebook_id</param>
				<param name="clientConfig" xsi:type="array">
					<item name="saveUrl" xsi:type="url" path="lofmp_facebooklivechat/Sellerfacebook/inlineEdit"/>
					<item name="validateBeforeSave" xsi:type="boolean">false</item>
				</param>
			</editorConfig>
			<childDefaults>
				<param name="fieldAction" xsi:type="array">
					<item name="provider" xsi:type="string">lofmp_facebooklivechat_sellerfacebook_listing.lofmp_facebooklivechat_sellerfacebook_listing.lofmp_facebooklivechat_sellerfacebook_columns_editor</item>
					<item name="target" xsi:type="string">startEdit</item>
					<item name="params" xsi:type="array">
						<item name="0" xsi:type="string">${ $.$data.rowIndex }</item>
						<item name="1" xsi:type="boolean">true</item>
					</item>
				</param>
			</childDefaults>
		</settings>
		<selectionsColumn name="ids">
			<settings>
				<indexField>sellerfacebook_id</indexField>
			</settings>
		</selectionsColumn>
		<column name="sellerfacebook_id">
			<settings>
				<filter>text</filter>
				<sorting>asc</sorting>
				<label translate="true">ID</label>
			</settings>
		</column>
		<column name="seller_id">
			<settings>
				<filter>text</filter>
				<label translate="true">Seller ID</label>
				<editor>
					<editorType>text</editorType>
					<validation>
						<rule name="required-entry" xsi:type="boolean">false</rule>
					</validation>
				</editor>
			</settings>
		</column>
		<column name="page_id">
			<settings>
				<filter>text</filter>
				<label translate="true">Facebook Page Id</label>
				<editor>
					<editorType>text</editorType>
					<validation>
						<rule name="required-entry" xsi:type="boolean">false</rule>
					</validation>
				</editor>
			</settings>
		</column>
		<column name="logged_in_greeting">
			<settings>
				<filter>text</filter>
				<label translate="true">Logged In Greeting</label>
				<editor>
					<editorType>text</editorType>
					<validation>
						<rule name="required-entry" xsi:type="boolean">false</rule>
					</validation>
				</editor>
			</settings>
		</column>
		<column name="logged_out_greeting">
			<settings>
				<filter>text</filter>
				<label translate="true">Logged Out Greeting</label>
				<editor>
					<editorType>text</editorType>
					<validation>
						<rule name="required-entry" xsi:type="boolean">false</rule>
					</validation>
				</editor>
			</settings>
		</column>
		<column name="theme_color">
			<settings>
				<filter>text</filter>
				<label translate="true">Theme Color</label>
				<editor>
					<editorType>text</editorType>
					<validation>
						<rule name="required-entry" xsi:type="boolean">false</rule>
					</validation>
				</editor>
			</settings>
		</column>
		<column name="created_at">
			<settings>
				<filter>text</filter>
				<label translate="true">created_at</label>
			</settings>
		</column>
		<column name="updated_at">
			<settings>
				<filter>text</filter>
				<label translate="true">updated_at</label>
			</settings>
		</column>
		<column name="status">
			<settings>
				<filter>text</filter>
				<label translate="true">Status</label>
			</settings>
		</column>

        <actionsColumn name="actions" class="Lofmp\FacebookLiveChat\Ui\Component\Listing\Column\SellerfacebookActions">
			<settings>
				<indexField>sellerfacebook_id</indexField>
				<resizeEnabled>false</resizeEnabled>
				<resizeDefaultWidth>107</resizeDefaultWidth>
			</settings>
		</actionsColumn>
	</columns>
</listing>
